// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:/.output/prisma/sqlite.db"
}

model Account {
  id       String @id @default(uuid())
  username String @unique
}

model Subscription {
  id  String @id @default(uuid())
  url String @unique
}
